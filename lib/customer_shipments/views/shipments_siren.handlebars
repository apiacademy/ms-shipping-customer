{
    "class" : ["{{ title }}"],

    "entities": [
    {{#each shipments}}
        {
        "class": ["customer"],
        "rel": ["item"],
        "href": "{{../base_url}}/shipments/{{{number}}}"
        }{{^last}},{{/last}}
    {{/each}}
    ],
    "links": [
    {{#each affordances}}
        {
        "rel": ["{{rel}}"], "href": "{{../base_url}}{{url}}"
        }{{^last}},{{/last}}
    {{/each}}
    ]
}